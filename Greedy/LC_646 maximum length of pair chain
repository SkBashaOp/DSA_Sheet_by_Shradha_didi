class Solution:
    def findLongestChain(self, pairs: List[List[int]]) -> int:
        pairs.sort(key=lambda x: x[1])
        n=len(pairs)
        if n<2:
            return n
        count=1
        val=pairs[0][1]
        for i in range(1,n):
            if(pairs[i][0]>val):
                count+=1
                val=pairs[i][1]
        return count            
