class Solution:
	def findLargest(self, nums):
        for i,n in enumerate(nums):
            nums[i]=str(n)
        def compare(n1,n2):
            if(n1+n2>n2+n1):
                return -1
            else:
                return 1        
        nums.sort(key=cmp_to_key(compare)) 
        result = ''.join(nums)
        if result[0] == '0':
            return '0'
        return result
